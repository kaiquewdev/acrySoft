//App

//Group of tabs
var tabGroup = Ti.UI.createTabGroup();

//Main Window
var main = Ti.UI.createWindow({
    title:  'AcrySof',
    url:    'main.js'
});

//Tab for app
var mainTab = Ti.UI.createTab({
    title:  'AcrySof',
    font-size: '16',
    window: main
});

//Add's
tabGroup.addTab(mainTab);
tabGroup.open();
